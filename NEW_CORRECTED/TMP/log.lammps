variable        x equal "cos((2*PI)*v_j/v_jmax)*(v_i/v_imax)*v_r*(0.2)"
variable        z equal "sin((2*PI)*v_j/v_jmax)*(v_i/v_imax)*v_r*(0.2)"

compute         1 all damage/atom
