variable count loop ${countmax}
variable count loop 2325

print "imax = ${imax}"
imax = 30
print "jmax = ${jmax}"
jmax = 1
print "kmax = ${kmax}"
kmax = 1
print "lmax = ${lmax}"
lmax = 1

# START LOOPS
# Loop 1
label start_of_loop_1
variable l loop ${lmax}
variable l loop 1

# Loop 2
label start_of_loop_2
variable k loop ${kmax}
variable k loop 1

# Loop 3
